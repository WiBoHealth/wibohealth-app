{
  "version": 2,
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/en/:path*",
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/nl/:path*",
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/nl",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
